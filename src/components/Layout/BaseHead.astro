---
import '../../styles/global.css';

export interface Props {
  title: string;
}

const { title } = Astro.props;
---
<meta charset="UTF-8" />
<meta name="description" content="DevFest Cali, 2 de Diciembre de 2023">
<meta name="viewport" content="width=device-width" />
<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
<meta name="generator" content={Astro.generator} />
<meta name="google" content="notranslate" />
<meta name="robots" content="all" />


<title>{title}</title>

<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:opsz,wght@6..12,300;6..12,400;6..12,700&family=Roboto:wght@100;400;700&display=swap&family=Roboto+Mono&display=swap" rel="stylesheet">